<aside id="sidebar">
	<nav>
		<% if current_user %>
			<ul>
				<% if current_user_admin? %>
					<li id="admin">(Admin Page)</li>
					<br>
				<% end %>
				<li class="button"><%= link_to "Add New Series", new_series_path %></li>
				<% unless current_page?(series_index_path) %>
					<li class="button"><%= link_to "All Series", series_index_path %></li>
				<% end %>
			<%= content_for(:side) %>
			</ul>
		<% else %>
			<p></p>
		<% end %>
	</nav>
</aside>
