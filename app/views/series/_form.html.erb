<%= form_for(@series) do |f| %>
	<%= render 'shared/errors', object: @series %>
	<fieldset id="series-form">
		<p>
			<%= f.label :name, "Series Name" %>
			<br>
      <%= f.text_field :name, autofocus: true, id:"name-field" %>
		</p>
		<p>
			<%= f.submit %> | 
	    <%= link_to "Cancel", series_index_path %>
	   </p>
	</fieldset>
<% end %>

